	<xsd:element name="STATEMENT_OF_TRANSACTIONS">
		<xsd:annotation>
			<xsd:documentation>Справка о движении ценных бумаг</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="party_dtls_contacts_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls" type="account_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Номер счета зарегистрированного лица (по которому предоставляется справка)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация о зарегистрированном лице, по счету которого предоставляется справка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="preparation_datetime" type="datetime_t">
							<xsd:annotation>
								<xsd:documentation>Дата составления справки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="start_date" type="date_t">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="date_t">
							<xsd:annotation>
								<xsd:documentation>Дата окончания периода</xsd:documentation>
							</xsd:annotation>
