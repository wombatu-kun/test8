															<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="code_base_transactions" type="code_base_transactions_et">
														<xsd:annotation>
															<xsd:documentation>код основания операций</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="previous_closing_balance" type="quantity_in_unit_t">
														<xsd:annotation>
															<xsd:documentation>исходящий остаток после глобальной операции</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="processing_info">
														<xsd:annotation>
															<xsd:documentation>Данные регистратора об операции</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="processing_datetime" type="date_or_datetime_t">
																	<xsd:annotation>
																		<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
																<xsd:element name="processing_reference" type="text_t">
																	<xsd:annotation>
																		<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
																		<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
																	</xsd:annotation>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="register_allocation" minOccurs="0" maxOccurs="unbounded">
														<xsd:annotation>
															<xsd:documentation>Реестр распределения</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="shareholder_id" type="id_t">
																	<xsd:annotation>
																		<xsd:documentation>Индивидуальный код лица,  присвоенный НДЦД (идентификационный номер  владельца)</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
																<xsd:element name="total" type="quantity_in_unit_t">
																	<xsd:annotation>
																		<xsd:documentation>количество ценных бумаг, подлежащих зачислению/списанию каждому владельцу</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="future_state">
											<xsd:annotation>
												<xsd:documentation>сведения о ЦБ которые будут учитываться на лицевом счете НДЦД после проведения глобальной операции</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="future_security" type="security_info_t">
														<xsd:annotation>
															<xsd:documentation>Ценные бумаги, после глобальной операции</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="future_opening_balance" type="quantity_in_unit_t">
														<xsd:annotation>
															<xsd:documentation>входящий остаток до глобальной операции</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="transaction_type" type="transaction_type_et">
														<xsd:annotation>
															<xsd:documentation>Вид операции относительно лицевого счета НДЦД</xsd:documentation>
															<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="code_base_transactions" type="code_base_transactions_et">
														<xsd:annotation>
															<xsd:documentation>код основания операций</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="future_closing_balance" type="quantity_in_unit_t">
														<xsd:annotation>
															<xsd:documentation>исходящий остаток после глобальной операции</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="processing_info">
														<xsd:annotation>
															<xsd:documentation>Данные регистратора об операции</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="processing_datetime" type="date_or_datetime_t">
																	<xsd:annotation>
																		<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
																<xsd:element name="processing_reference" type="text_t">
