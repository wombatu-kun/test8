	<xsd:element name="GLOBAL_OPERATION_STATUS_ADVICE">
		<xsd:annotation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
			<xsd:documentation>Подтверждение сверки при глобальной/комплексной операции</xsd:documentation>
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД. Номер счета обязательно заполняется.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты номинального держателя центрального депозитария</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="counterparty" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Контрагент НДЦД. Обязательно заполняется для Лица, выкупающего ценные бумаги по ст. 84.8 Закона об АО </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
										<xsd:annotation>
											<xsd:documentation>информация о счете контрагента</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="account_holder_v02" type="party_identification_t">
										<xsd:annotation>
											<xsd:documentation>информация о контрагенте</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="global_operation_status" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>сведения о глобальной операции, по которой центральным депозитарием проведена сверка, с указанием результатов сверки </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="security" type="security_info_t">
										<xsd:annotation>
											<xsd:documentation>Ценные бумаги</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="processing_info">
										<xsd:annotation>
											<xsd:documentation>Данные регистратора об операции</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:annotation>
													<xsd:documentation>может отсутствовать при отказе</xsd:documentation>
												</xsd:annotation>
												<xsd:element name="processing_datetime" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_reference" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
														<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="reconciliation_info" type="reconciliation_info_t">
										<xsd:annotation>
											<xsd:documentation>информация о результатах проведенной сверки с указанием причины отказа в свекре по операции</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CORPORATE_ACTION_NOTIFICATION">
		<xsd:annotation>
			<xsd:documentation>Уведомление о глобальной операции/ Требование на составление списка</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД. Номер счета обязательно заполняется.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты номинального держателя центрального депозитария</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer_V02" type="party_identification2_t" maxOccurs="2">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="base_security" type="security_info_t" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>базовые бумаги корпоративного действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="future_security" type="security_info_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>информация о новых выпусках ценных</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="new_issuer" type="party_identification2_t" minOccurs="0" maxOccurs="2">
							<xsd:annotation>
								<xsd:documentation>Информация о втором эмитенте, заполняется в случае, если корпоративное действие связано с реорганизацией</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice>
							<xsd:element name="corporate_action_date" type="date_t">
								<xsd:annotation>
									<xsd:documentation>дата (предполагаемая дата) проведения корпоративного действия</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:sequence>
								<xsd:element name="register_date" type="date_t">
									<xsd:annotation>
										<xsd:documentation>дата составления списка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="security" type="security_info_t" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>ценные бумаги, по которым составляется список; если ценные бумаги не укзаны, список составляется по всем ЦБ, числящимся на счете НДЦД</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:choice>
						<xsd:element name="block_date" type="date_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>дата, на которую необходимо заблокировать проведение операций с ценными бумагами реорганизуемого общества</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_code" type="corporate_action_event_et">
							<xsd:annotation>
								<xsd:documentation>Вид корпоративного действия/ события</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_narrative" type="text_t">
							<xsd:annotation>
								<xsd:documentation>текстовое описание корпоративного действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_reference" type="text_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Референс корпоративного события. уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам, затрагивающим финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации эмитентами эмиссионных ценных бумаг.</xsd:documentation>
								<!--==========================================================================================================
								уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам , затрагивающим
_______________финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации
_______________ эмитентами эмиссионных ценных бумаг.
								============================================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="buyer_info" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сведения о лице, выкупающем ценные бумаги по ст. 84.8 Закона об АО </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="account_dtls" type="account_dtls_t">
										<xsd:annotation>
											<xsd:documentation>Номер счета и тип лицевого счета</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="shareholder_dtls" type="party_dtls_t">
										<xsd:annotation>
											<xsd:documentation>основная информация о выкупающем лице</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="legal_address" type="address_t" nillable="true" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>адрес места нахождения</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="post_address" type="address_t" nillable="true" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>почтовый адрес</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<xsd:element name="REPORT_OF_UNCONFIRMED_TRANSACTION_MIF">
		<xsd:annotation>
			<xsd:documentation>Запрос сверки при выдаче /погашении / обмене паев</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="previous_ref" type="ref_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Указание на предыдущий запрос сверки по указанному ЛСНДЦД. В отсутствие сведений, блок  previous_ref заполняется следующим образом:
- ref_doc_num = NONREF
- ref_doc_date = 1900-01-01 (т.н. условная дата)
</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты НДЦД</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t">
							<xsd:annotation>
								<xsd:documentation>сведения о выдаваемых/погашаемых инвестиционных паях/ сертификатах участия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_linked_instructions" type="number_t">
							<xsd:annotation>
								<xsd:documentation>общее число связанных операций в пакете</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="transactions_info_mif" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>операции по счету номинального держателя центрального депозитария с паями, по которым необходимо провести сверку</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="current_instruction_number" type="number_t">
										<xsd:annotation>
											<xsd:documentation>порядковый номер операции в пределах массива операций. Значение поля  определяет последовательность сверки операций центральным депозитарием.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="transaction_type" type="transaction_type_et">
										<xsd:annotation>
											<xsd:documentation>Вид операции относительно лицевого счета НДЦД</xsd:documentation>
											<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="code_base_transactions" type="code_base_transactions_et">
										<xsd:annotation>
											<xsd:documentation>код основания операций</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="doc_link_request">
										<xsd:annotation>
											<xsd:documentation>Референс  - Реквизиты заявки на приобретение/погашение инвестиционных паев.</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="doc_num" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер заявки</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="doc_date" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>дата заявки</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="management_comp_account" type="account_dtls_v02_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>счет УК ЗПИФ</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="quantity" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>количество ценных бумаг, участвующих в операции</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="block_indicator" type="yes_no_t">
										<xsd:annotation>
											<xsd:documentation>Указание на наличие обременения ценных бумаг.</xsd:documentation>
											<!-- ===================================================
 передаточном распоряжении допускается использование только кода (No) Данный тип электронного документа не используется для передачи обремененных ценных бумаг
=================================================== -->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="block_type" type="block_type_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Вид обременения ценных бумаг. </xsd:documentation>
											<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="opening_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>входящий остаток до выдачи/погашения инвестиционных паев</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="closing_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>исходящий остаток после выдачи/погашения инвестиционных паев</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="processing_info">
										<xsd:annotation>
											<xsd:documentation>Данные об обработке документа</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="link" type="doc_link_t">
													<xsd:annotation>
														<xsd:documentation>информация об инициирующем документе.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_datetime" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_reference" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
														<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="owner" type="text_t" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>указание имени (наименования) лица, которое является (при списании с л/с подателя поручения) или будет являться (при зачислении на л/с подателя поручения) владельцем (доверительным управляющим) паев, заявка которого исполнена</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SETTLEMENT_TRANSACTION_STATUS_ADVICE_MIF">
		<xsd:annotation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
			<xsd:documentation>Подтверждение исполненной операции при выдаче /погашении / обмене паев / Уведомления о списании паев в связи с обменом </xsd:documentation>
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД. Номер счета обязательно заполняется.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты номинального держателя центрального депозитария</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_linked_instructions" type="number_t">
							<xsd:annotation>
								<xsd:documentation>общее число связанных операций в пакете</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t">
							<xsd:annotation>
								<xsd:documentation>Сведения о выдаваемых/погашаемых инвестиционных паях, по которым проведена сверка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="mif_operation_status" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>сведения об операции, по которой центральным депозитарием проведена сверка, с указанием результатов сверки </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="current_instruction_number" type="number_t">
										<xsd:annotation>
											<xsd:documentation>порядковый номер операции в пределах массива операций. Значение поля  определяет последовательность сверки операций центральным депозитарием.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="code_base_transactions" type="code_base_transactions_et">
										<xsd:annotation>
											<xsd:documentation>код основания операций</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="transaction_type" type="transaction_type_et">
										<xsd:annotation>
											<xsd:documentation>Вид операции относительно лицевого счета НДЦД</xsd:documentation>
											<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="processing_info">
										<xsd:annotation>
											<xsd:documentation>Данные регистратора об операции</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="link" type="doc_link_t">
													<xsd:annotation>
														<xsd:documentation>информация об инициирующем документе.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_datetime" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_reference" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
														<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="reconciliation_info" type="reconciliation_info_t">
										<xsd:annotation>
											<xsd:documentation>информация о результатах проведенной сверки с указанием причины отказа в сверке по операции</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="doc_link_request" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Реквизиты заявки на приобретение/погашение/ обмен инвестиционных паев. Заполняется в случае confirmed_status=RJCT</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="doc_num" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер заявки</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="doc_date" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>дата заявки</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="shareholder_info" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Реквизиты лица, указанного в заявке на приобретение/ погашение/ обмен паев. Заполняется в случае confirmed_status=RJCT</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="shareholder_dtls" type="party_dtls_t">
													<xsd:annotation>
														<xsd:documentation>основная информация о зарегистрированном лице</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="manager_info" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Ф.И.О. руководителя (указывается только для юридических лиц)</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="inn" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>код ИНН</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="iin" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>код ИИН</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ssn" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Номер страхового свидетельства</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="kpp" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>код КПП</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="okpo" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>код ОКПО</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="okved" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>код ОКВЭД</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="birthday" type="date_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Дата рождения</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="nationality" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Гражданство</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="postal_name" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>наименование  юридического лица или ФИО  физического лица, и адрес для направления корреспонденции</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="name" type="text_t">
																<xsd:annotation>
																	<xsd:documentation>Наименование юридического лица или ФИО физического лица</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="address" type="address_t">
																<xsd:annotation>
																	<xsd:documentation>адрес для направления корреспонденции</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATUS_CHANGE_INSTRUCTION">
		<xsd:annotation>
			<xsd:documentation>Распоряжение на изменение статуса ценных бумаг по лицевому счету номинального держателя центрального депозитария </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете. Номер счета обязательно заполняется.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>Информация о зарегистрированном лице (по которому предоставляется распоряжение)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t">
							<xsd:annotation>
								<xsd:documentation>информация о ценной бумаге</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="code_base_transactions" type="code_base_transactions_et">
							<xsd:annotation>
								<xsd:documentation>код основания изменения статуса ЦБ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="previous_security_status">
							<xsd:annotation>
								<xsd:documentation>статус и количество ЦБ до изменения</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="quantity" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг указанного статуса</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="security_status" type="block_type_t">
										<xsd:annotation>
											<xsd:documentation>Статус ценных бумаг</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="registration_date" type="date_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>дата регистрации в реестре/ изменения статуса</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="future_security_status">
							<xsd:annotation>
								<xsd:documentation>статус и количество ЦБ после изменения</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="quantity" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг указанного статуса</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="security_status" type="block_type_t">
										<xsd:annotation>
											<xsd:documentation>Статус ценных бумаг</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<xsd:element name="REPORT_OF_UNCONFIRMED_STATUS_CHANGE">
		<xsd:annotation>
			<xsd:documentation>Запрос сверки операции, не связанной с движением ценных бумаг (изменением статуса ценных бумаг)</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="previous_ref" type="ref_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Указание на предыдущий запрос сверки по указанному ЛСНДЦД. В отсутствие сведений, блок  previous_ref заполняется следующим образом:
- ref_doc_num = NONREF
- ref_doc_date = 1900-01-01 (т.н. условная дата)
</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты НДЦД</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t">
							<xsd:annotation>
								<xsd:documentation>Ценная бумага</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total" type="quantity_in_unit_t">
							<xsd:annotation>
								<xsd:documentation>Общее количество ценных бумаг, числящихся на счете НДЦД</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_linked_instructions" type="number_t">
							<xsd:annotation>
								<xsd:documentation>общее число связанных операций в пакете</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="transaction_info" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>сведения об операциях по изменению статуса ЦБ на счете НДЦД</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="current_instruction_number" type="number_t">
										<xsd:annotation>
											<xsd:documentation>порядковый номер операции в пределах массива операций. Значение поля  определяет последовательность сверки операций центральным депозитарием.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="previous_security_status">
										<xsd:annotation>
											<xsd:documentation>статус и количество ЦБ до изменения</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="quantity" type="quantity_in_unit_t">
													<xsd:annotation>
														<xsd:documentation>Количество ценных бумаг указанного статуса</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="security_status" type="block_type_t">
													<xsd:annotation>
														<xsd:documentation>Статус ценных бумаг</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="future_security_status">
										<xsd:annotation>
											<xsd:documentation>статус и количество ЦБ после изменения</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="quantity" type="quantity_in_unit_t">
													<xsd:annotation>
														<xsd:documentation>Количество ценных бумаг указанного статуса</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="security_status" type="block_type_t">
													<xsd:annotation>
														<xsd:documentation>Статус ценных бумаг</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="processing_info">
										<xsd:annotation>
											<xsd:documentation>Данные регистратора об операции</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="doc_link_csd" type="doc_link_t">
													<xsd:annotation>
														<xsd:documentation>Реквизиты поручения НДЦД; При отсутствии инициирующего документа, например, при корпоративных действиях, блок заполняется следующим образом:
out_doc_num = NONREF
out_doc_date\date = 1900-01-01 (т.н. условная дата)</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_datetime" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_reference" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
														<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="OPERATION_STATUS_CONFIRMATION">
		<xsd:annotation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
			<xsd:documentation>Подтверждение операции, не связанной с движением ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете НДЦД. Номер счета обязательно заполняется.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты номинального держателя центрального депозитария</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t">
							<xsd:annotation>
								<xsd:documentation>Ценная бумага</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_linked_instructions" type="number_t">
							<xsd:annotation>
								<xsd:documentation>общее число связанных операций в пакете</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reconciliation_status" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>информация о результатах проведенной сверки по всем операциям по указанной ценной бумаге</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="processing_info">
										<xsd:annotation>
											<xsd:documentation>Данные регистратора об операции</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="doc_link_csd" type="doc_link_t">
													<xsd:annotation>
														<xsd:documentation>Реквизиты поручения НДЦД; При отсутствии инициирующего документа, например, при корпоративных действиях, блок заполняется следующим образом:
out_doc_num = NONREF
out_doc_date\date = 1900-01-01 (т.н. условная дата)</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_datetime" type="date_or_datetime_t">
													<xsd:annotation>
														<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="processing_reference" type="text_t">
													<xsd:annotation>
														<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
														<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="reconciliation_info" type="reconciliation_info_t">
										<xsd:annotation>
											<xsd:documentation>информация о результатах проведенной сверки с указанием причины отказа в свекре по операции</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REGISTER_OF_SHAREHOLDERS_HEADER">
		<xsd:annotation>
			<xsd:documentation>Список владельцев - заголовок</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="party_id_name_t" maxOccurs="2">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте; Для паев ПИФ и ИСУ формируется два блока issuer. Для УК указывается ОГРН, в id/issuer = OGRN. Для ПИФ указывается номер правил ДУ,  в id/issuer = TMRL. Для остальных финансовых инструментов указывается ОГРН Эмитента, в id/issuer = OGRN.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls" type="account_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Номер счета, по которому предоставляется информация. Лицевой счет номинального держателя в реестре </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация о зарегистрированном лице, по счету которого предоставляется отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_code" type="corporate_action_event_et">
							<xsd:annotation>
								<xsd:documentation>Вид корпоративного действия/ события</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_reference" type="text_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Референс корпоративного события. Уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам , затрагивающим финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации эмитентами эмиссионных ценных бумаг.</xsd:documentation>
								<!--===================================================================================
								уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам , затрагивающим
_______________финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации
_______________ эмитентами эмиссионных ценных бумаг.
								=====================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="record_date" type="date_t">
							<xsd:annotation>
								<xsd:documentation>Дата, на окончание операционного дня, которой составлены данные</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="message_function" type="message_function_et">
							<xsd:annotation>
								<xsd:documentation>Назначение предоставляемой информации. Первичный список - NEWM, дораскрытие - AMND</xsd:documentation>
								<!--=================================================================================
								первичный список - NEWM, дораскрытие - AMND
								====================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="information_indicator" type="information_type_t">
							<xsd:annotation>
								<xsd:documentation>Тип предоставляемой информации (тип списка)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_link" type="doc_link_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Связанные ссылки (сообщения) </xsd:documentation>
								<!--==================================================================================
								Указываются  регистрационные номера ранее предоставленных сообщений. Повторяющийся элемент.
________________ Может быть указана ссылка на полученное уведомление о корпоративном событии. 
________________При дораскрытии указывается ссылка на ранее предоставленный список.
								====================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="register_list_info">
							<xsd:annotation>
								<xsd:documentation>Список владельцев </xsd:documentation>
								<!--==================================================================================
								блок повторяющийся  при дораскрытии ранее предоставленного списка
								=====================================================================================-->
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="page_total">
										<xsd:annotation>
											<xsd:documentation>количество сообщений, в которых передается список</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="register_list_id">
										<xsd:annotation>
											<xsd:documentation>идентификатор списка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<xsd:element name="REGISTER_OF_SHAREHOLDERS_LIST">
		<xsd:annotation>
			<xsd:documentation>Список владельцев - содержание</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="register_list_id">
							<xsd:annotation>
								<xsd:documentation>идентификатор списка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="page_total">
							<xsd:annotation>
								<xsd:documentation>количество сообщений, в которых передается список</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="page">
							<xsd:annotation>
								<xsd:documentation>порядковый номер сообщения в списке</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="register_list" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список владельцев </xsd:documentation>
								<!--==================================================================================
								блок повторяющийся  при дораскрытии ранее предоставленного списка
								=====================================================================================-->
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="shareholder_id" type="id_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Индивидуальный код указанного в блоке shareholder лица,  присвоенный номинальным держателем, предоставившим ранее список.</xsd:documentation>
											<!--===============================================================================
											Используется, как идентификатор shareholder при дораскрытии нераскрытого остатка на счете номинального держателя в ранее предоставленном списке. Заполняется только в случае дораскрытия списка. Повторяющийся элемент. 
											=================================================================================-->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="shareholder" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Блок информации о владельце ценных бумаг </xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="shareholder_id" type="id_t">
													<xsd:annotation>
														<xsd:documentation>Индивидуальный код указанного в блоке shareholder лица,  присвоенный номинальным держателем, предоставляющим данные</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="account_dtls" type="account_dtls_t">
													<xsd:annotation>
														<xsd:documentation>Информация о счете</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="shareholder_info">
													<xsd:annotation>
														<xsd:documentation>Полная информация о зарегистрированном лице</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="shareholder_dtls" type="party_dtls_t">
																<xsd:annotation>
																	<xsd:documentation>основная информация о зарегистрированном лице</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="manager_info" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Ф.И.О. руководителя (указывается только для юридических лиц)</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="inn" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>код ИНН</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="iin" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>код ИИН</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="ssn" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Номер страхового свидетельства</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="kpp" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>код КПП</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="okpo" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>код ОКПО</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="okved" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>код ОКВЭД</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="birthday" type="date_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Дата рождения</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="nationality" type="text_t" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Гражданство</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="postal_name" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>наименование  юридического лица или ФИО  физического лица, и адрес для направления корреспонденции</xsd:documentation>
																</xsd:annotation>
																<xsd:complexType>
																	<xsd:sequence>
																		<xsd:element name="name" type="text_t">
																			<xsd:annotation>
																				<xsd:documentation>Наименование юридического лица или ФИО физического лица</xsd:documentation>
																			</xsd:annotation>
																		</xsd:element>
																		<xsd:element name="address" type="address_t">
																			<xsd:annotation>
																				<xsd:documentation>адрес для направления корреспонденции</xsd:documentation>
																			</xsd:annotation>
																		</xsd:element>
																	</xsd:sequence>
																</xsd:complexType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="shareholder_contacts" type="party_contacts_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>контактная информация (номера телефонов, факс)</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="tax_category" type="tax_category_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>статус налогоплательщика</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="bank_prop_rub" type="bank_prop_rub_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Банковские реквизиты стороны (указываются банковские реквизиты получателя дохода по ценным бумагам)</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="authorization_info" type="text_t" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Основание для получения дохода. Может заполняться для указания информации об основании получения доходов. в случае, если получатель дохода не совпадает с владельцем ценных бумаг – может быть указана например, информация о договоре счета депо, которым депозитарий уполномочен получать доходы,  и др. информация </xsd:documentation>
														<!--==========================================================================================================
														Может заполняться для указания информации об основании получения доходов. в случае, если получатель дохода не совпадает с владельцем ценных бумаг – может быть указана например, информация о договоре счета депо, которым депозитарий уполномочен получать доходы,  и др. информация
													============================================================================================================-->
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="security_balance" type="security_balance_t" maxOccurs="unbounded">
													<xsd:annotation>
														<xsd:documentation>Блок информации об остатках по ценной бумаге </xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<xsd:element name="CORPORATE_ACTION_CANCELLATION_REQUEST">
		<xsd:annotation>
			<xsd:documentation>Запрос на отмену действия, связанного с корпоративным событием/ Запрос на отмену сбора списка владельцев</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="cancellation_request_reference" type="ref_dtls_t">
							<xsd:annotation>
								<xsd:documentation>указание реквизитов распоряжения, подлежащего отмене (из блока header)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>Информация о счете</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>реквизиты владельца счета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer_V02" type="party_identification2_t" maxOccurs="2">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security" type="security_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>информация о ценной бумаге</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_reference" type="text_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Референс корпоративного события. уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам, затрагивающим финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации эмитентами эмиссионных ценных бумаг.</xsd:documentation>
								<!--==========================================================================================================
								уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам , затрагивающим
_______________финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации
_______________ эмитентами эмиссионных ценных бумаг.
								============================================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_narrative" type="text_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>текстовое описание корпоративного действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_code" type="corporate_action_event_et">
							<xsd:annotation>
								<xsd:documentation>Вид корпоративного действия/ события</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<!-- =================================================== -->
	<!-- 5. СПРАВОЧНИК XML - структур
 	                                                                         СЛОВАРЬ ТИПОВ ДАННЫХ-->
	<!-- =================================================== -->
	<xsd:simpleType name="date_t">
		<xsd:annotation>
			<xsd:documentation>дата; при отсутствии реальных данных необходимо указывать следующее значение:1900-01-01</xsd:documentation>
			<!-- ========================================================================
YYYY-MM-DD - При отсутствии реальных данных необходимо указывать следующее значение:
1900-01-01
=========================================================================== -->
		</xsd:annotation>
		<xsd:restriction base="xsd:date"/>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="datetime_t">
		<xsd:annotation>
			<xsd:documentation>дата и время; при отсутствии реальных данных необходимо указывать следующее значение: 1900-01-01T00:00:00</xsd:documentation>
			<!-- ========================================================================
YYYY-MM-DDThh:mm:ss - При отсутствии реальных данных необходимо указывать следующее значение:
1900-01-01T00:00:00
=========================================================================== -->
		</xsd:annotation>
		<xsd:restriction base="xsd:dateTime"/>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="decimal_t">
		<xsd:annotation>
			<xsd:documentation>десятичное число (18,16); при отсутствии реальных данных необходимо указывать следующее значение: 00.00</xsd:documentation>
			<!-- ========================================================================
При отсутствии реальных данных необходимо указывать следующее значение:
00.00
=========================================================================== -->
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="number_t">
		<xsd:annotation>
			<xsd:documentation>целое число; при отсутствии реальных данных необходимо указывать следующее значение: 00</xsd:documentation>
			<!-- ========================================================================
При отсутствии реальных данных необходимо указывать следующее значение:
00
=========================================================================== -->
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="0"/>
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="text_t">
		<xsd:annotation>
			<xsd:documentation>произвольная строка текста; при отсутствии реальных данных необходимо указывать следующее значение: UKWN</xsd:documentation>
			<!-- ========================================================================
При отсутствии реальных данных необходимо указывать следующее значение:
UKWN
=========================================================================== -->
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="yes_no_t">
		<xsd:annotation>
			<xsd:documentation>признак "Да" (Yes)/ "Нет" (No)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="text_t">
			<xsd:enumeration value="Yes">
				<xsd:annotation>
					<xsd:documentation>Yes - Да</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="No">
				<xsd:annotation>
					<xsd:documentation>No - Нет</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<!-- ===================================================
	СЛОВАРЬ XML- составных типов
	=================================================== -->
	<xsd:complexType name="account_dtls_t">
		<xsd:annotation>
			<xsd:documentation>подробная информация о счете/ лицевом счете зарегистрированного лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_id" type="id_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>идентификатор (номер) счета; указание номера счета является обязательным при наличии открытого счета и при проведении операций с участием НДЦД</xsd:documentation>
					<!-- ===================================================
Указание номера счета является обязательным при наличии открытого счета
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_type" type="account_type_et">
				<xsd:annotation>
					<xsd:documentation>тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="account_dtls_v02_t">
		<xsd:annotation>
			<xsd:documentation>подробная информация о счете/ лицевом счете</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_id" type="id_t">
				<xsd:annotation>
					<xsd:documentation>Номер счета, обязательно указывается в случае проведения операций с участием счета НДЦД (при наличии ) </xsd:documentation>
					<!-- ===================================================
Указание номера счета является обязательным при наличии открытого счета
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_type" type="account_type_t">
				<xsd:annotation>
					<xsd:documentation>тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="account_type_t">
		<xsd:annotation>
			<xsd:documentation>тип счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_type_code" type="account_type_et">
				<xsd:annotation>
					<xsd:documentation>код типа счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание типа счета, подлежит обязательному заполнению при account_type = 13</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="address_partad_t">
		<xsd:annotation>
			<xsd:documentation>структура адреса в формате ПАРТАД</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="country" type="text_t">
				<xsd:annotation>
					<xsd:documentation>код страны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="index" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>почтовый индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" type="text_t">
				<xsd:annotation>
					<xsd:documentation>адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="address_t">
		<xsd:annotation>
			<xsd:documentation>обобщенный адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="partad" type="address_partad_t">
					<xsd:annotation>
						<xsd:documentation>адрес в структуре ПАРТАД</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="kladr" type="text_t" minOccurs="0" maxOccurs="0">
					<xsd:annotation>
						<xsd:documentation>адрес по КЛАДР</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="plain" type="text_t">
					<xsd:annotation>
						<xsd:documentation>Не структурированный почтовый адрес</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="answer_doc_t">
		<xsd:annotation>
			<xsd:documentation>общие элементы для всех ответных документов/ сообщений, т.е. документов, которые являются ответом на чей-либо запрос. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="version" type="text_t" fixed="FCDR_13_01" nillable="false">
				<xsd:annotation>
					<xsd:documentation>версия XML документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="header">
				<xsd:annotation>
					<xsd:documentation>Заголовок документа</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="doc_num" type="text_t">
							<xsd:annotation>
								<xsd:documentation>исходящий номер ответного документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_date" type="date_or_datetime_t">
							<xsd:annotation>
								<xsd:documentation>дата создания ответного документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="link" type="doc_link_t">
							<xsd:annotation>
								<xsd:documentation>информация об инициирующем документе. В отсутствие инициирующего документа, блок link заполняется следующим образом:
- out_doc_num = NONREF
- out_doc_date\date = 1900-01-01 (т.н. условная дата)
</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- ======================================================================================= -->
	</xsd:complexType>
	<xsd:complexType name="any_t" mixed="true">
		<xsd:annotation>
			<xsd:documentation>неконтролируемый блок для расширений. Элемент может содержать произвольную информацию: текст, другие элементы, двоичную информацию (графику, цифровые подписи, звук, …) и т.д.</xsd:documentation>
			<!-- ===================================================
Элемент может содержать произвольную информацию: текст, другие элементы, двоичную информацию (графику, цифровые подписи, звук, …) и т.д.
=================================================== -->
		</xsd:annotation>
		<xsd:sequence>
			<xsd:any processContents="skip" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Неконтролируемый блок для расширений. Элемент может содержать произвольную информацию: текст, другие элементы, двоичную информацию (графику, цифровые подписи, звук, …) и т.д.. </xsd:documentation>
				</xsd:annotation>
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="bank_prop_rub_t">
		<xsd:annotation>
			<xsd:documentation>Банковские реквизиты сторон для расчетов в рублях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="pay_name" type="text_t">
				<xsd:annotation>
					<xsd:documentation>Наименование  юридического лица или ФИО  физического лица. стороны (плательщика/ получателя) платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ИНН стороны (плательщика/ получателя)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cash_rub_dtls" type="cash_rub_dtls_t">
				<xsd:annotation>
					<xsd:documentation>информация о банковских реквизитах в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pay_add_info" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные реквизиты платежа (данные которые необходимы в поле «Назначение платежа»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="based_info_t">
		<xsd:annotation>
			<xsd:documentation>основание - список договоров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="contract_type" type="contract_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>Номер договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_date" type="date_t">
				<xsd:annotation>
					<xsd:documentation>Дата договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="block_type_t">
		<xsd:annotation>
			<xsd:documentation>вид обременения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="block_type_code" type="block_type_et">
				<xsd:annotation>
					<xsd:documentation>код вида обременения/ блокировки</xsd:documentation>
					<!-- ===================================================
Код вида обременения, должен принимать значения из фиксированного перечня. 
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текстовое наименование вида обременения / блокировки, заполняется только при  block_type_code=OTHR. Заполнение narrative при указание иных кодов в блоке block_type_code (отличного от OTHR)  является основанием для отказа в обработке документа.</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида обременения указано OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="restrictions" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сведения об обременении / блокировке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledgee_info" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сведения о залоге, обязательно заполняется при  block_type_code=COLO</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Информация о счете (при его наличии)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="pledgee" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация о залогодержателе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="pledgee_narrative" type="text_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Описание условий залога</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="cash_rub_dtls_t">
		<xsd:annotation>
			<xsd:documentation>информация о банковских реквизитах в рублях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account" type="id_t">
				<xsd:annotation>
					<xsd:documentation>счет №/ номер лицевого счета стороны</xsd:documentation>
					<!-- ===================================================
Cч № (номер лицевого счета стороны (плательщика/ получателя) в банке
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_name" type="text_t">
				<xsd:annotation>
					<xsd:documentation>наименование банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_city" type="text_t">
				<xsd:annotation>
					<xsd:documentation>город банка</xsd:documentation>
					<!-- ===================================================
Город банка стороны (плательщика/ получателя)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ruic" type="text_t">
				<xsd:annotation>
					<xsd:documentation>БИК банка</xsd:documentation>
					<!-- ===================================================
БИК банка стороны (плательщика/ получателя) - банковский идентификационный код, присваиваемый Банком России кредитным организациям
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_corr" type="id_t">
				<xsd:annotation>
					<xsd:documentation>счет №/ номер корреспондентского счета</xsd:documentation>
					<!-- ===================================================
					Cч №  (номер корреспондентского счета банка стороны (плательщика/ получателя) в одном из расчетных учреждений (подразделений) Банка России
				=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="contract_type_t">
		<xsd:annotation>
			<xsd:documentation>вид договора/ иного основания</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="contract_code" type="contract_et">
				<xsd:annotation>
					<xsd:documentation>код вида договора.</xsd:documentation>
					<!-- ===================================================
Бзаполняется в соответствии со справочником кодов видов договоров/ иных оснований
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание наименование договора/ иного основания. Заполняется обязательно в случае, если значением кода вида договора/ иного основания указан код OTHR.</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида договора/ иного основания указан код OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="currency_and_amount_t">
		<xsd:annotation>
			<xsd:documentation>валюта и сумма</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ccy_code" type="text_t">
				<xsd:annotation>
					<xsd:documentation>Код валюты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="amount" type="decimal_t">
				<xsd:annotation>
					<xsd:documentation>сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="date_or_datetime_t">
		<xsd:annotation>
			<xsd:documentation>дата или дата и время. </xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="date" type="date_t">
				<xsd:annotation>
					<xsd:documentation>Дата</xsd:documentation>
					<!-- ===================================================
Обязательно заполняется один из двух элементов
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="datetime" type="datetime_t">
				<xsd:annotation>
					<xsd:documentation>Дата и время</xsd:documentation>
					<!-- ===================================================
Обязательно заполняется один из двух элементов
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="doc_link_t">
		<xsd:annotation>
			<xsd:documentation>связанные ссылки (информация об инициирующем документе). В отсутствие инициирующего документа, блок link заполняется следующим образом:
	out_doc_num = NONREF
	out_doc_date\date = 1900-01-01 (т.н. условная дата)</xsd:documentation>
			<!--=======================================================
в отсутствие инициирующего документа, блок link заполняется следующим образом:
	out_doc_num = NONREF
	out_doc_date\date = 1900-01-01 (т.н. условная дата)
===================================================================-->
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="out_doc_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>исходящий номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_doc_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>ссылка на входящий регистрационный номер полученного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="out_doc_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата создания исходящего документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_reg_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата регистрации входящего документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="entity_reg_doc_type_t">
		<xsd:annotation>
			<xsd:documentation>тип документа о регистрации юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="entity_reg_doc_type_code" type="entity_reg_doc_type_et">
				<xsd:annotation>
					<xsd:documentation>код типа документа о регистрации юридического лица</xsd:documentation>
					<!-- ===================================================
Тип документа, должен принимать значения из фиксированного перечня
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание наименования типа документа/ вид лицензии. Заполняется обязательно, только в случае, если значением кода типа документа указано OTHR.</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода типа документа указано OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="entity_reg_dtls_t">
		<xsd:annotation>
			<xsd:documentation>информация о регистрации юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="reg_doc_type" type="entity_reg_doc_type_t">
				<xsd:annotation>
					<xsd:documentation>тип документа о регистрации юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>номер регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_doc_part" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>серия документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="date_of_incorporation" type="date_t">
				<xsd:annotation>
					<xsd:documentation>дата регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_org" type="text_t">
				<xsd:annotation>
					<xsd:documentation>орган, осуществивший регистрацию</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="id_t">
		<xsd:annotation>
			<xsd:documentation>идентификационная информация</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="text_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор. При указании номера счета поля issuer и scheme_name могут иметь любое значение или не указываеться.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="issuer" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Указывается код Организации, ведущей справочник идентификационных кодов, не зарегистрированная SWIFTом (NSDR – НКО НРД), если иное правило заполнения поля не задано выше лежащими блоками XSD-схемы.</xsd:documentation>
						<!-- ===================================================
Должен быть указан один из элементов (issuer или scheme_name)
Могут указываться первые 4 буквы BIC-кода SWIFT организации
FCSM – ФСФР (Федеральная служба по финансовым рынкам)
NSDR – НКО НРД
DCLC – ДКК
OTHR – прочие
Если код не известен может быть указано UKWN
=================================================== -->
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="text_t"/>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="scheme_name" type="scheme_name_et" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Наименование схемы кодировки – для организаций, зарегистрированных SWIFT</xsd:documentation>
						<!-- ===================================================
Должен быть указан один из элементов (issuer или scheme_name)
Могут указываться коды, организаций, включенных в справочник Data Source Scheme, используемый SWIFT
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="id_type_t">
		<xsd:annotation>
			<xsd:documentation>идентификатор с указанием типа идентификатора</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="text_t">
				<xsd:annotation>
					<xsd:documentation>При необходимости указать в идентификаторе серию и номер, сперва указывается серия, потом ставится пробел, потом номер.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="id_type" type="id_type_et">
				<xsd:annotation>
					<xsd:documentation>тип идентификатора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>неструктурированное описание типа идентификатора, обязательно заполняется при id_type=OTHR</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="individual_document_t">
		<xsd:annotation>
			<xsd:documentation>документ, удостоверяющий личность физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_type" type="individual_document_type_t">
				<xsd:annotation>
					<xsd:documentation>тип документа, удостоверяющего личность физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_ser" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>серия документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_date" type="date_t">
				<xsd:annotation>
					<xsd:documentation>когда выдан</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="org" type="text_t">
				<xsd:annotation>
					<xsd:documentation>орган, выдавший документ</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, выдавшего документ, удостоверяющего личность
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="place" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>место выдачи документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- ============================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="individual_document_type_t">
		<xsd:annotation>
			<xsd:documentation>тип документа, удостоверяющего личность физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="individual_document_type_code" type="individual_document_type_et">
				<xsd:annotation>
					<xsd:documentation>код типа документа, удостоверяющего личность физического лица</xsd:documentation>
					<!-- ===================================================
Принимает значения из фиксированного перечня значений (справочника).
Перечень значений определяется в соответствии со «Справочником «Виды документов, удостоверяющих личность налогоплательщика», приведенного в Приложении к формам N 1-НДФЛ и 2-НДФЛ «Справочники», утвержденном Приказом МНС России от 14.07.2004 N САЭ-3-13/423@ «Об утверждении форм отчетности по налогу на доходы физических лиц».
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание типа документа, заполняется обязательно только в случае, если значением типа документа указано 00</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением типа документа указано 00
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="information_type_t">
		<xsd:annotation>
			<xsd:documentation>тип информации/ списка</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="information_type_code" type="information_indicator_et">
				<xsd:annotation>
					<xsd:documentation>код типа информации/ списка</xsd:documentation>
					<!-- ===================================================
Принимает значения из фиксированного перечня (справочника).
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание типа информации/наименования списка; подлежит обязательному заполнению, если значение кода равно OTHR</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значение кода типа информации равно OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="out_doc_t">
		<xsd:annotation>
			<xsd:documentation>общие элементы документов, инициируемых участником обмена</xsd:documentation>
			<!-- ===================================================
Блок содержит поля, общие для всех документов, инициируемых участником обмена.
=================================================== -->
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="version" type="text_t" fixed="FCDR_13_01" nillable="false">
				<xsd:annotation>
					<xsd:documentation>версия XML документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="header">
				<xsd:annotation>
					<xsd:documentation>заголовок документа</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="doc_num">
							<xsd:annotation>
								<xsd:documentation>номер документа по системе учета отправителя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_date" type="date_or_datetime_t">
							<xsd:annotation>
								<xsd:documentation>дата документа по системе учета отправителя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="party_contacts_t">
		<xsd:annotation>
			<xsd:documentation>контактная информация о стороне</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="phone_or_fax" type="phone_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>номер телефона</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="e_mail" type="text_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>адрес электронной почты</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="party_dtls_contacts_t">
		<xsd:annotation>
			<xsd:documentation>характеристики стороны с контактами</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="party_dtls" type="party_dtls_t">
				<xsd:annotation>
					<xsd:documentation>характеристики стороны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contacts" type="party_contacts_t">
				<xsd:annotation>
					<xsd:documentation>контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="party_dtls_t">
		<xsd:annotation>
			<xsd:documentation>характеристики стороны </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="id_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>идентификация стороны (код) </xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="text_t">
				<xsd:annotation>
					<xsd:documentation>полное наименование юридического лица/ Ф.И.О. физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="short_name" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>краткое наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ogrn" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ОГРН  /основной государственный регистрационный номер </xsd:documentation>
					<!-- ===================================================
обязателен при наличии
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" type="address_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual_or_entity" type="individual_or_entity_et">
				<xsd:annotation>
					<xsd:documentation>Признак юридического/ физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="entity_reg_dtls" type="entity_reg_dtls_t" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>информация о регистрации - для юридического лица</xsd:documentation>
						<!-- ===================================================
Один из блоков
Обязательно для юрического лица – код субъекта – LEGL
повторяющийся элемент -допускается указание нескольких документов о регистрации юридического лица (например, свидетельствоо регистрации в МРП и о внесвении записи в ЕГРЮЛ)
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="individual_document" type="individual_document_t">
					<xsd:annotation>
						<xsd:documentation>информация о регистрации физического лица</xsd:documentation>
						<!-- ===================================================
Один из блоков
Обязательно для физического лица – код субъекта - INDV
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<!-- ============================================================================================= -->
	</xsd:complexType>
	<xsd:complexType name="party_id_name_t">
		<xsd:annotation>
			<xsd:documentation>краткая идентификация и наименование стороны</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="id_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>идентификация стороны</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="text_t">
				<xsd:annotation>
					<xsd:documentation>наименование стороны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="party_identification_t">
		<xsd:annotation>
			<xsd:documentation>идентификационная информация лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_2" type="id_type_t">
				<xsd:annotation>
					<xsd:documentation>идентификатор с указанием типа идентификатора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="id_date" type="date_t">
				<xsd:annotation>
					<xsd:documentation>дата присвоения (получения) идентификатора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="party_identification2_t">
		<xsd:annotation>
			<xsd:documentation>идентификационная информация лица (вариант 2)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_2" type="id_type_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Регистрационные данные </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="postal_address" type="address_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>почтовый адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="party_contacts" type="party_contacts_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="phone_t">
		<xsd:annotation>
			<xsd:documentation>номер телефона</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="phone_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>не структурированный номер телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="phone_type" type="phone_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код типа номера телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- ============================================================================================= -->
	</xsd:complexType>
	<xsd:complexType name="processing_dtls_t">
		<xsd:annotation>
			<xsd:documentation>информация об обработке документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_link" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Ссылка на документ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transaction" type="transaction_t">
				<xsd:annotation>
					<xsd:documentation>описание/ информация о сделке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_info">
				<xsd:annotation>
					<xsd:documentation>Данные об обработке документа</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:annotation>
							<xsd:documentation>может отсутствовать при отказе</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="operation_name" type="text_t">
							<xsd:annotation>
								<xsd:documentation>Наименование операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="processing_datetime" type="date_or_datetime_t">
							<xsd:annotation>
								<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="processing_reference" type="text_t">
							<xsd:annotation>
								<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
								<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="processing_dtls_v02_t">
		<xsd:annotation>
			<xsd:documentation>сведения об исполнении операции в реестре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="current_instruction_number" type="number_t">
				<xsd:annotation>
					<xsd:documentation>порядковый номер операции в пределах массива операций. Значение поля  определяет последовательность сверки операций центральным депозитарием.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_link_csd" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Реквизиты поручения НДЦД; При отсутствии инициирующего документа, например, при корпоративных действиях, блок заполняется следующим образом:
out_doc_num = NONREF
out_doc_date\date = 1900-01-01 (т.н. условная дата)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_link_allegement" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Реквизиты встречного поручения; При отсутствии инициирующего документа, например, при корпоративных действиях, блок заполняется следующим образом:
out_doc_num = NONREF
out_doc_date\date = 1900-01-01 (т.н. условная дата)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_reference" type="text_t">
				<xsd:annotation>
					<xsd:documentation>порядковый номер внесения записи об операции по регистрационному журналу Регистратора</xsd:documentation>
					<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_datetime" type="date_or_datetime_t">
				<xsd:annotation>
					<xsd:documentation>дата внесения записи об операции по регистрационному журналу Регистратора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="processing_dtls_v03_t">
		<xsd:annotation>
			<xsd:documentation>информация об обработке документа версия 3</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_link" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Ссылка на документ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_info">
				<xsd:annotation>
					<xsd:documentation>Данные об обработке документа</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:annotation>
							<xsd:documentation>может отсутствовать при отказе</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="operation_name" type="text_t">
							<xsd:annotation>
								<xsd:documentation>Наименование операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="processing_datetime" type="date_or_datetime_t">
							<xsd:annotation>
								<xsd:documentation>Дата обработки документа/ дата операции/ дата внесения записи в реестр</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="processing_reference" type="text_t">
							<xsd:annotation>
								<xsd:documentation>номер документа /номер операции/ номер внесения записи в реестр</xsd:documentation>
								<!-- ===================================================
Референс обработки документа /номер операции/ номер внесения записи в реестр
=================================================== -->
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="reconciliation_info_t">
		<xsd:annotation>
			<xsd:documentation>информация о результатах проведенной сверки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="confirmed_status" type="confirmed_status_et">
				<xsd:annotation>
					<xsd:documentation>Подтверждение об успешной сверке (CONF) либо Отказ в сверке (RJCT) по конкретной ценной бумаге (при проведении операции по счету НДЦД или при ежедневной сверке). Если указано значение (PEND - Сверка не проводилась), то для проведения сверки требуются дополнительные сведения, указанные в reason_narrative_rejected</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="confirmed_date" type="datetime_t">
				<xsd:annotation>
					<xsd:documentation>дата и время проведения сверки по конкретной ценной бумаге в системе ЦД</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="difference_size" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>величина расхождения; обязательно заполняется, если confirmed_status равен RJCT;
равно нулю или отсутствие тэга - расхождение данных по операции или количеству учитываемых на счете НДЦД ценных бумаг отсутствует;
отрицательное значение – количество ЦБ, числящихся по учету НДЦД, меньше количества ЦБ, числящихся по учету регистратора;
положительное значение – количество ЦБ, числящихся по учету НДЦД, больше количества ЦБ, числящихся по учету регистратора.</xsd:documentation>
					<!--differences_size =0 или отсутствие тэга  – расхождение данных по операции или количеству учитываемых на счете НДЦД ценных бумаг отсутствует
differences_size < 0 (отрицательное значение) – количество ЦБ, числящихся по учету НДЦД, меньше количества ЦБ, числящихся по учету регистратора
differences_size > 0 (положительное значение) – количество ЦБ, числящихся по учету НДЦД, больше количества ЦБ, числящихся по учету регистратора.
-->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reason_narrative_rejected" type="text_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>неструктурированное описание причины отказа в проведении сверки по операции/по ценной бумаге, заполняется в случае  confirmed_status = PEND при невозможности проведения свекри по конкретной операции/ ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ref_dtls_t">
		<xsd:annotation>
			<xsd:documentation>ссылка на связанный документ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ref_doc_num" type="text_t">
				<xsd:annotation>
					<xsd:documentation>номер связанного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ref_doc_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата или дата и время связанного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ref_name" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>наименование связанного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="quantity_in_unit_t" mixed="true">
		<xsd:annotation>
			<xsd:documentation>количество ценных бумаг в штуках</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="units" type="decimal_t">
				<xsd:annotation>
					<xsd:documentation>Целая часть и десятичная дробь. При наличии fraction - только целое</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fraction" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дробная часть (подблок). Заполняется только в случае простой дроби. В случае десятичной дроби не заполняется</xsd:documentation>
					<!-- ===================================================
Заполняется только в случае простой дроби
В случае десятичной дроби не заполняется
=================================================== -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="numerator" type="number_t">
							<xsd:annotation>
								<xsd:documentation>Числитель</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="denominator" type="number_t">
							<xsd:annotation>
								<xsd:documentation>Знаменатель</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="reject_reason_type_t">
		<xsd:annotation>
			<xsd:documentation>формализованная причина отказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="processing_rejected" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>отказ в обработке поручения/инструкции</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="processing_rejected_reason" type="processing_rejected_reason_et">
								<xsd:annotation>
									<xsd:documentation>причина присвоения статуса обработки поручения инструкции</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="cancellation_rejected" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>отказ в обработке поручения на отмену</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="cancellation_rejected_reason" type="cancellation_rejected_reason_et">
								<xsd:annotation>
									<xsd:documentation>причина присвоения статуса обработки поручения на отмену</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="unmatched" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>статус квитовки - НЕ сквитовано</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="unmatched_reason" type="unmatched_reason_et">
								<xsd:annotation>
									<xsd:documentation>причина присвоения статуса "НЕ сквитовано"</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="request_rejected">
					<xsd:annotation>
						<xsd:documentation>отказ в обработке информационного запроса</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="request_rejected_reason" type="request_rejected_reason_et">
								<xsd:annotation>
									<xsd:documentation>причина присвоения статуса  </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="signature_t">
		<xsd:annotation>
			<xsd:documentation>блок для описания электронной подписи</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="signature_ISO_SRO" type="SignatureType"/>
			<xsd:element name="signature_ISO" type="SignatureEnvelope"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="security_balance_t">
		<xsd:annotation>
			<xsd:documentation>баланс ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="total" type="quantity_in_unit_t">
				<xsd:annotation>
					<xsd:documentation>Общее количество ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blocked" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Блок заполняется только при наличии обременений и блокировок ЦБ. Указание в блоке quantity нулевых значений может являться основанием для отказа в обработке документа</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="quantity" type="quantity_in_unit_t">
							<xsd:annotation>
								<xsd:documentation>Количество ценных бумаг</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="block_type" type="block_type_t" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Вид обременения. </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="security_info_t">
		<xsd:annotation>
			<xsd:documentation>информация о ценной бумаге</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="security_classification" type="security_classification_et">
				<xsd:annotation>
					<xsd:documentation>Вид ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_classification_CFI" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код вида ценной бумаги (CFI)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="financial_instrument_type" type="financial_instrument_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код вида финансового инструмента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_category" type="security_category_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Категория ценной бумаги по предоставляемым правам</xsd:documentation>
					<!-- ===================================================
Заполняется только в случае, если вид ценной бумаги – SHAR –Акция
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_type" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nominal_value" type="currency_and_amount_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номинал</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="state_reg_num">
				<xsd:annotation>
					<xsd:documentation>Государственный регистрационный номер, номер правил ДУ ПИФ</xsd:documentation>
					<!-- ===================================================
Если регистратор ведет учет нескольких выпусков с  одним государственным регистрационным номером в разрезе отдельных выпусков (по дате регистрации), рекомендуется в обязательном порядке указывать дату государственной регистрации
=================================================== -->
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="text_t">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="state_reg_date" type="date_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата государственной регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ISIN_identifier" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код ISIN ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="proprietary_security_code" type="id_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Частный идентификационный код ценной бумаги</xsd:documentation>
					<!-- ===================================================
Зарезервировано для последующего использования
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="status_reason_dtls_t">
		<xsd:annotation>
			<xsd:documentation>статусы и причины присвоения статусов обработки документов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="processing_status">
					<xsd:annotation>
						<xsd:documentation>статус обработки поручения/ инструкции</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:choice>
								<xsd:element name="accepted">
									<xsd:annotation>
										<xsd:documentation>документ получен и принят к обработке</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="processing_rejected" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>иные статусы обработки поручения/инструкции</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="processing_rejected_reason" type="processing_rejected_reason_et">
												<xsd:annotation>
													<xsd:documentation>причина присвоения статуса обработки поручения инструкции</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="cancellation_processing_status">
					<xsd:annotation>
						<xsd:documentation>статус обработки поручения на отмену</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:choice>
								<xsd:element name="accepted">
									<xsd:annotation>
										<xsd:documentation>запрос на отмену получен и принят к дальнейшей обработке</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="completed">
									<xsd:annotation>
										<xsd:documentation>отмена завершена</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="cancellation_rejected" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>иные статусы обработки поручения на отмену</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="cancellation_rejected_reason" type="cancellation_rejected_reason_et">
												<xsd:annotation>
													<xsd:documentation>причина присвоения статуса обработки поручения на отмену</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="matching_status">
					<xsd:annotation>
						<xsd:documentation>статус квитовки</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:choice>
								<xsd:element name="matched">
									<xsd:annotation>
										<xsd:documentation>статус квитовки - сквитовано</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины квитовки</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="unmatched" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>статус квитовки - НЕ сквитовано</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="unmatched_reason" type="unmatched_reason_et">
												<xsd:annotation>
													<xsd:documentation>причина присвоения статуса "НЕ сквитовано"</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<xsd:element name="reason_narrative" type="text_t" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>неструктурированное описание причины, обязательно заполняется, если "_reason" = NARR</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tax_category_t">
		<xsd:annotation>
			<xsd:documentation>статус налогоплательщика</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tax_status_code" type="tax_status_et">
				<xsd:annotation>
					<xsd:documentation>Код статуса налогоплательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nonresident_type" type="nonresident_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип нерезидента (юридического лица)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_exempt_indicator" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак наличия налоговой льготы</xsd:documentation>
					<!-- ===================================================
Yes – в случае наличия льготы
No – при отсутствии льготы.
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание для налоговой льготы. </xsd:documentation>
					<!-- ===================================================
Возможно указание наименования  ПИФа, № лицензии управляющей компании, даты выдачи, органа, выдавшего лицензию. 
Для иностранных банков могут быть указаны идентифицирующие сведения из общедоступных источников
================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="transaction_t">
		<xsd:annotation>
			<xsd:documentation>описание/ информация о сделке/ операции</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="transaction_type" type="transaction_type_et">
				<xsd:annotation>
					<xsd:documentation>Вид операции </xsd:documentation>
					<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="quantity_in_unit_t">
				<xsd:annotation>
					<xsd:documentation>Количество ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_indicator" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Указание на наличие обременения.</xsd:documentation>
					<!-- ===================================================
В передаточном распоряжении допускается использование только кода (No) Данный тип электронного документа не используется для передачи обремененных ценных бумаг
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_type" type="block_type_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вид обременения. </xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="settlement_amount" type="currency_and_amount_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма сделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contragent">
				<xsd:annotation>
					<xsd:documentation>Контрагент</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="account_dtls" type="account_dtls_t">
							<xsd:annotation>
								<xsd:documentation>информация о счете контрагента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>информация о контрагенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание.
					(повторяющийся элемент)
Если одна из сторон сделки является НДЦД (account_type= «12»)
обязательно заполняются:
	для указания референса встречных поручений для проведения операции по счету НДЦД в реестре, используется следующее правило:
•	в поле transaction\ based_info\ contract_type\ contract_code указывается код «OTHR»
•	в поле transaction\ based_info\ contract_type\ narrative указывается идентификатор common_id
•	в поле transaction\ based_info\ doc_num указывается значение референса</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
Если одна из сторон сделки является НДЦД (account_type= «12»)
обязательно заполняются:
	для указания референса встречных поручений для проведения операции по счету НДЦД в реестре, используется следующее правило:
•	в поле transaction\ based_info\ contract_type\ contract_code указывается код «OTHR»
•	в поле transaction\ based_info\ contract_type\ narrative указывается идентификатор common_id
•	в поле transaction\ based_info\ doc_num указывается значение референса
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="transaction_v02_t">
		<xsd:annotation>
			<xsd:documentation>описание/ информация о сделке/ операции</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="transaction_type" type="transaction_type_et">
				<xsd:annotation>
					<xsd:documentation>Вид операции (направление движения ЦБ. Определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет</xsd:documentation>
					<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="code_base_transactions" type="code_base_transactions_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код основания операций</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="quantity_in_unit_t">
				<xsd:annotation>
					<xsd:documentation>Количество ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_indicator" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Указание на наличие обременения.</xsd:documentation>
					<!-- ===================================================
 передаточном распоряжении допускается использование только кода (No) Данный тип электронного документа не используется для передачи обремененных ценных бумаг
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_type" type="block_type_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вид обременения. </xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="settlement_amount" type="currency_and_amount_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма сделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contragent">
				<xsd:annotation>
					<xsd:documentation>Контрагент</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="account_dtls" type="account_dtls_t">
							<xsd:annotation>
								<xsd:documentation>информация о счете контрагента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>информация о контрагенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции.
Запрещается в качестве одного из оснований перевода ценных бумаг указывать договор купли-продажи (иной сделки).</xsd:documentation>
					<!-- ===================================================
Если одна из сторон сделки является НДЦД (account_dtls \account_type= «12» или transaction \ contragent \account_dtls \account_type= «12»)
обязательно заполняются:
	для указания референса встречных поручений для проведения операции по счету НДЦД в реестре, используется следующее правило:
•	в поле transaction\ based_info\ contract_type\ contract_code указывается код «OTHR»
•	в поле transaction\ based_info\ contract_type\ narrative указывается идентификатор CmonId
•	в поле transaction\ based_info\ doc_num указывается значение референса
Запрещается в качестве одного из оснований перевода ценных бумаг указывать договор купли-продажи (иной сделки).
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="common_id" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Референс поручения (идентификационный номер встречных распоряжений). Обязательно заполняется при проведении операции с участием НДЦД. Используется для выполнения квитовки поручений в реестре.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="owner" type="text_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>указание имени (наименования) лица, которое является (при списании со л/с подателя поручения) или будет являться (при зачислении на л/с подателя поручения) владельцем (доверительным управляющим) ценных бумаг. Обязательно заполняется при проведении операции с участием НДЦД.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="transaction_v03_t">
		<xsd:annotation>
			<xsd:documentation>описание/ информация о сделке/ операции</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="common_id" type="text_t">
				<xsd:annotation>
					<xsd:documentation>Референс поручения (идентификационный номер встречных распоряжений). Используется для выполнения квитовки поручений в реестре.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transaction_type" type="transaction_type_et">
				<xsd:annotation>
					<xsd:documentation>Вид операции относительно лицевого счета НДЦД</xsd:documentation>
					<!-- ===================================================
 (определяется по отношению к счету, по которому предоставляется передаточное распоряжение или выдается отчет)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="code_base_transactions" type="code_base_transactions_et">
				<xsd:annotation>
					<xsd:documentation>код основания операций</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание кода основания операции при code_base_transactions =   TR39 (ИНОЕ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="quantity_in_unit_t">
				<xsd:annotation>
					<xsd:documentation>количество ценных бумаг, участвующих в операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_indicator" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Указание на наличие обременения ценных бумаг.</xsd:documentation>
					<!-- ===================================================
 передаточном распоряжении допускается использование только кода (No) Данный тип электронного документа не используется для передачи обремененных ценных бумаг
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_type" type="block_type_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вид обременения / блокировки ценных бумаг. Блок заполняется только при наличии обременений или блокировок ЦБ. Указание в блоке quantity нулевых значений может являться основанием для отказа в обработке документа.</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contragent">
				<xsd:annotation>
					<xsd:documentation>Контрагент НДЦД</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="account_dtls_v02" type="account_dtls_v02_t">
							<xsd:annotation>
								<xsd:documentation>информация о счете контрагента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder_v02" type="party_identification_t">
							<xsd:annotation>
								<xsd:documentation>информация о контрагенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="owner" type="text_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>указание имени (наименования) лица, которое является (при списании с л/с подателя поручения) или будет являться (при зачислении на л/с подателя поручения) владельцем (доверительным управляющим) ценных бумаг. Обязательно заполняется при проведении операции с участием НДЦД. В случае выкупа и реализации эмитентом размещенных им ценных бумаг, а также приобретения или выкупа акций в связи с осуществлением добровольного, в том числе конкурирующего, или обязательного предложения в соответствии с главой XI.1 ФЗ «Об акционерных обществах» выкупа каждом из встречных распоряжений , поданных Регистратору, должно быть указано имя (наименование) лица, отчуждающего ценные бумаги.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="corporate_action_reference" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Референс корпоративного события</xsd:documentation>
					<!--==========================================================================================================
								уникальный код, присваивается в соответствии с «Порядком присвоения кодов существенным фактам , затрагивающим
_______________финансово-хозяйственную деятельность эмитента ценных бумаг», приложение 23 к Положению о раскрытии информации
_______________ эмитентами эмиссионных ценных бумаг.
								============================================================================================================-->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- =================================================== -->
	<!--СОСТАВНЫЕ ТИПЫ 2 ЭТАП-->
	<xsd:complexType name="content_list_of_security_t">
		<xsd:annotation>
			<xsd:documentation>содержание документа "Справочник по ценным бумагам"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SN" type="number_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Порядковый номер выпуска </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_org" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование регистрирующего органа, осуществившего государственную регистрацию выпуска ценных бумаг </xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_info" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>характеристики ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vote" type="number_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количество голосов, предоставляемое акцией (для привилегированных акций) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="form_issue" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Форма выпуска ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dividend_rate" type="decimal_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Размер дивиденда (ставка процента)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_in_issue" type="quantity_in_unit_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количество ценных бумаг в выпуске</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_in_place" type="quantity_in_unit_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количество размещенных ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="state_report_date" type="date_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата утверждения отчета об итогах выпуска ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="shareholder_t">
		<xsd:annotation>
			<xsd:documentation>блок информации о лице, которому открыт (открывается) счет</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="shareholder_dtls" nillable="false">
				<xsd:annotation>
					<xsd:documentation>характеристики  лица</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="org" type="organisation_t"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="shareholder_contacts" type="party_contacts_t" nillable="false">
				<xsd:annotation>
					<xsd:documentation>контактная информация (номера телефонов, факс)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_info" type="tax_category_t">
				<xsd:annotation>
					<xsd:documentation>сведения о налогообложении</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="letter_go_type" type="letter_go_type_et">
				<xsd:annotation>
					<xsd:documentation>Способ получения информации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cash_settlement_dtls" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>денежные расчеты (банковские реквизиты)</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="cash_settlement_type" type="cash_settlement_type_et" nillable="true">
							<xsd:annotation>
								<xsd:documentation>тип денежных расчетов (банковских реквизитов)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="bank_prop_rub" type="bank_prop_rub_t" nillable="true">
							<xsd:annotation>
								<xsd:documentation>реквизиты для денежных расчетов</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="organisation_t">
		<xsd:annotation>
			<xsd:documentation>характеристики организации</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="organisation_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>полное наименование организации</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="text_t">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="organisation_short_name" type="text_t" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>краткое наименование организации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="organisation_other_name" type="text_t" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>иное наименование организации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="entity_reg_dtls" type="entity_reg_dtls_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>сведения о документах организации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="legal_address" type="address_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>адрес места нахождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="post_address" type="address_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>почтовый адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registration_country" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>страна регистрации (инкорпорации) юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ИНН</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="text_t"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="iin" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ИИН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ssn" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер страхового свидетельства</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="kpp" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код КПП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="okpo" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ОКПО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="okved" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ОКВЭД</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="manager" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>единоличный исполнительный орган, обязательно для юридического лица</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="manager_type" type="text_t" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Должность единоличного исполнительного органа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="FIO" type="text_t" nillable="false">
							<xsd:annotation>
								<xsd:documentation>Фамилия, Имя, Отчество</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="individual_document" type="individual_document_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Документ, удостоверяющий личность физического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="business_application_header_t">
		<xsd:annotation>
			<xsd:documentation>блок служебной информации</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="from" type="party_identification2_t">
				<xsd:annotation>
					<xsd:documentation>отправитель сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="to" type="party_identification2_t">
				<xsd:annotation>
					<xsd:documentation>получатель сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="business_message_identifier" type="text_t">
				<xsd:annotation>
					<xsd:documentation>идентификатор сообщения для его однозначной идентификации стороной, создавшей прилагаемое бизнес-сообщение </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="message_definition_identifier" type="text_t">
				<xsd:annotation>
					<xsd:documentation>указание на наименование корневого элемента; допускается указание только наименований корневых элементов, включенных в согласованную схему документов, например, REQUEST_FOR_STATEMENT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="business_service" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>указание на получение бизнес-услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="creation_date" type="date_t">
				<xsd:annotation>
					<xsd:documentation>дата создания сообщения; в общем случае дата создания заголовка бизнес-сообщения не равна дате создания самого бизнес-сообщения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="copy_duplicate_code" type="copy_duplicate_code_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>указание на вид (назначение) сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="possible_duplicate" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак дупликата. Поле указывает на то, что передаваемое сообщение, возможно, является дубликатом ранее переданного сообщения.
Если получатель сообщения не получал оригинал сообщения, он должен обрабатывать передаваемое сообщение так, как будто оно является оригинальным. Если же получатель сообщения ранее получил его оригинал, то он должен предпринять меры для предотвращения повторной обработки.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="priority" type="text_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак приоритета. Поле указывает, должен ли передаваемый документ быть исполнен с высоким приоритетом.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="signature" type="signature_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>электронная подпись</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- =================================================== -->
	<!--6.	СПРАВОЧНИК ГЛОБАЛЬНЫХ КОДОВ XML-элементов 
							(СЛОВАРИ)-->
	<!-- =================================================== -->
	<xsd:simpleType name="account_type_et">
		<xsd:annotation>
			<xsd:documentation>тип счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="text_t">
			<xsd:enumeration value="01">
				<xsd:annotation>
					<xsd:documentation>счет владельца</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="02">
				<xsd:annotation>
					<xsd:documentation>счет номинального держателя</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="03">
				<xsd:annotation>
					<xsd:documentation>счет доверительного управляющего</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="04">
				<xsd:annotation>
					<xsd:documentation>счет залогодержателя</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="05">
				<xsd:annotation>
					<xsd:documentation>эмиссионный счет</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="06">
				<xsd:annotation>
					<xsd:documentation>казначейский счет эмитента (лица, обязанного по ценным бумагам)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="07">
				<xsd:annotation>
					<xsd:documentation>счет неустановленных лиц</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="12">
				<xsd:annotation>
					<xsd:documentation>счет номинального держателя центрального депозитария</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="13">
				<xsd:annotation>
					<xsd:documentation>иные счета, предусмотренные федеральными законами</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="18">
				<xsd:annotation>
					<xsd:documentation>счет доверительного управляющего правами</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="19">
				<xsd:annotation>
					<xsd:documentation>учет прав на ценные бумаги, переданные в депозит нотариуса или суда</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="20">
				<xsd:annotation>
					<xsd:documentation>счет иностранного номинального держателя</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="21">
				<xsd:annotation>
					<xsd:documentation>счет иностранного уполномоченного держателя</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="22">
				<xsd:annotation>
					<xsd:documentation>счет депозитарных программ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="23">
				<xsd:annotation>
					<xsd:documentation>счет "выдаваемые инвестиционные паи"</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="24">
				<xsd:annotation>
					<xsd:documentation>счет "дополнительные инвестиционные паи"</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="25">
				<xsd:annotation>
					<xsd:documentation>счет "выдаваемые ипотечные сертификаты участия"</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="26">
				<xsd:annotation>
					<xsd:documentation>счет "дополнительные ипотечные сертификаты участия"</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="base_document_et">
		<xsd:annotation>
			<xsd:documentation>тип документа - основания</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="text_t">
			<xsd:enumeration value="INST">
				<xsd:annotation>
					<xsd:documentation>передаточное распоряжение</xsd:documentation>
