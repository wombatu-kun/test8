						<xsd:element name="security_transactions" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Блок информации об операциях с ценными бумагами </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="security" type="security_info_t">
										<xsd:annotation>
											<xsd:documentation>Ценная бумага</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="opening_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг на начало периода.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="closing_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг на конец периода </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="transactions" type="processing_dtls_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Описание сделки/ операции  с ценными бумагами </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
				<!-- ===================================================
Корневой элемент (root element) XML-документа. Содержит все поля блока answer_doc_t.
=================================================== -->
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
	<xsd:element name="STATEMENT_OF_TRANSACTIONS_V02">
		<xsd:annotation>
			<xsd:documentation>Справка об операциях по лицевому счету</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="answer_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="party_dtls_contacts_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_dtls" type="account_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Номер счета зарегистрированного лица (по которому предоставляется справка)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_holder" type="party_dtls_t">
							<xsd:annotation>
								<xsd:documentation>Информация о зарегистрированном лице, по счету которого предоставляется справка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="preparation_datetime" type="datetime_t">
							<xsd:annotation>
								<xsd:documentation>Дата составления справки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="start_date" type="date_t">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="date_t">
							<xsd:annotation>
								<xsd:documentation>Дата окончания периода</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="transactions" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>информация обо всех операциях по лицевому счету за указанный период времени</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="opening_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг на начало периода.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="closing_balance" type="quantity_in_unit_t">
										<xsd:annotation>
											<xsd:documentation>Количество ценных бумаг на конец периода </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:choice>
										<xsd:element name="transactions_all" type="processing_dtls_t">
											<xsd:annotation>
												<xsd:documentation>Описание операции  с ценными бумагами по счету, за исключением счета НДЦД</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="transactions_ndcd">
											<xsd:annotation>
												<xsd:documentation>описание операции с ценными бумагами по счету номинального держателя центрального депозитария</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="security" type="security_info_t">
														<xsd:annotation>
															<xsd:documentation>Ценная бумага, участвующая в операции</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="transaction_v03" type="transaction_v03_t">
														<xsd:annotation>
															<xsd:documentation>описание деталей операции по лицевому счету НДЦД</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="processing_dtls_v02" type="processing_dtls_v02_t">
														<xsd:annotation>
															<xsd:documentation>сведения об исполнении операции в реестре по лицевому счету НДЦД</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="transactions_other">
											<xsd:annotation>
												<xsd:documentation>описание операции, не связанной с движением ценных бумаг</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="account_dtls_v02" type="account_dtls_v02_t" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>информация о счете контрагента</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="account_holder_v02" type="party_identification_t" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>информация о контрагенте</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="transaction_processing" type="processing_dtls_v03_t">
														<xsd:annotation>
															<xsd:documentation>информация об обработке документа (дата внесения записи в реестр и номер записи в регистрационном журнале об исполненной операции)</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:choice>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
	</xsd:element>
