					<xsd:documentation>закрытие счета</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="cash_settlement_type_et">
		<xsd:annotation>
			<xsd:documentation>тип денежных расчетов (зарезервировано для последующего использования)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="text_t">
			<xsd:enumeration value="DVCA">
				<xsd:annotation>
					<xsd:documentation>Дивиденды в денежной форме</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="confirmed_status_et">
		<xsd:annotation>
			<xsd:documentation>код результата сверки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="text_t">
			<xsd:enumeration value="CONF">
				<xsd:annotation>
					<xsd:documentation>Сверка проведена успешно</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RJCT">
				<xsd:annotation>
					<xsd:documentation>Отказ в проведении сверки</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PEND">
				<xsd:annotation>
					<xsd:documentation>Сверка не проводилась</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
